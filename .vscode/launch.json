{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "envFile": "${workspaceFolder}/.env",
            "preLaunchTask": "install-dependencies"
        },
        {
            "name": "트레이딩 스케줄러 테스트 (프로덕션)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/trading_scheduler_test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "DEV_MODE": "false"
            },
            "envFile": "${workspaceFolder}/.env",
            "preLaunchTask": "install-dependencies"
        },
        {
            "name": "라운드 실행",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "DEV_MODE": "true"
            },
            "envFile": "${workspaceFolder}/.env",
            "preLaunchTask": "install-dependencies"
        },
        {
            "name": "테스트 스캘핑 트레이더 실행",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/scalping_trader_test.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "DEV_MODE": "true"
            },
            "envFile": "${workspaceFolder}/.env",
            "preLaunchTask": "install-dependencies"
        }
    ]
} 